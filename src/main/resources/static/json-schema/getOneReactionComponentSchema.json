{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://localhost:8080/json-schema/getOneReactionComponentSchema.json",
  "$base": "https://localhost:8080/api/",
  "title": "Reaction Component Schema",
  "type": "object",
  "required": [ "data" ],
  "additionalProperties": false,
  "properties": {
    "data": {
      "data": {
        "type": "object",
        "required": [ "id", "type", "attribute" ],
        "additionalProperties": false,
        "properties": {
          "id": { "allOf": [{ "$ref": "common/id.json" }] },
          "type": {
            "const": "reactionComponent",
            "description": "The type of the object of the data being returned."
          },
          "attributes": { "allOf" : [{ "$ref": "definitions/reactioncomponent.json" }] },
          "relationships": {
            "properties": {

              "protocol": { "allOf": [{ "$ref": "common/relationship.json" }] }

            }
          },
          "links": { "allOf": [{ "$ref": "common/link.json" }] }
        }
      }
    }
  }
}