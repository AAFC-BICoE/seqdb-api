<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://local.xsd/dbchangelog-4.4.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="44-Change_SeqReaction_to_use_StorageUnitUsage" context="schema-change" author="cgendreau">
        <dropColumn tableName="seq_reaction">
            <column name="well_column"/>
            <column name="well_row"/>
        </dropColumn>
        <addColumn tableName="seq_reaction">
            <column name="storage_unit_usage" type="uuid"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>
